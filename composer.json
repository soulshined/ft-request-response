{
    "name": "ft/request-response",
    "description": "A library with common request response utils and helper methods",
    "type": "library",
    "version": "1.1.2",
    "license": "MIT",
    "keywords": [
        "request",
        "response",
        "server"
    ],
    "support": {
        "issues": "https://github.com/soulshined/ft-request-response/issues"
    },
    "authors": [
        {
            "name": "Freer",
            "email": "support@davidfreer.com"
        }
    ],
    "require": {
        "php" : ">=8.1"
    },
    "autoload": {
        "psr-4": {
            "FT\\RequestResponse\\": "src/",
            "FT\\RequestResponse\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "creativestyle/app-http-server-mock": "^1.0",
        "symfony/process": "^6.1",
        "guzzlehttp/guzzle": "^7.4",
        "symfony/http-foundation": "^6.1"
    },
    "scripts": {
        "test" : "phpunit tests --verbose"
    },
    "minimum-stability": "stable"
}
